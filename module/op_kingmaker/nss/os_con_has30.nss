/*
     Check if PC has thirty gold pieces.
*/
//:://////////////////////////////////////////////
//:: Created By: Cori
//:: Created On: Sept 2003
//:://////////////////////////////////////////////
int StartingConditional()
{
    object oPC = GetFirstPC();
    if (GetGold(oPC)>29)
        return TRUE;
    return FALSE;
}
